.outTime {
    font-size: 24rpx;
    color: #222834;
    line-height: 36rpx;
    margin-bottom: 14rpx;
}

.outTitle {
    display: block;
    width: 422rpx;
    font-size: 28rpx;
    color: #222834;
    line-height: 36rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.coach-info {
    float: right;
    display: flex;
    align-items: center;
    flex-wrap: nowrap;
    text-align: right;
}

.coach {
    font-size: 24rpx;
    color: #707784;
    line-height: 44rpx;
    display: inline-block;
}

.avatar {
    width: 40rpx;
    height: 40rpx;
    border: 1px solid #FFFFFF;
    border-radius: 50%;
    margin-right: 8rpx;
    display: inline-block;
}

.leftLine {
    display: flex;
    flex: 1;
    margin-top: -26rpx;
    margin-left: 8rpx;
    padding-bottom: 40rpx;
    position: relative;
}

.timeAsix {
    position: relative;
}


