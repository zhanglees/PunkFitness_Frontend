.itemTimeZ {
    display: flex;
    align-items: center;
    margin-bottom: 32rpx;
}

.leftContent {
    width: 80rpx;
}

.outRoundVIew {
    width: 16rpx;
    height: 16rpx;
    border-radius: 50%;
    background: #89A0EE;
    z-index: 1;
    position: relative;
    margin-left: 16rpx;
}

.timeAsix .itemTimeZ:first-child .leftLine {
    background: transparent;
}

.rightContent {
    flex: 1;
    margin-left: 64rpx;
}

.leftContent {
    font-size: 28rpx;
    font-family: Roboto-Medium;
    font-weight: 500;
    color: #222834;
    line-height: 36rpx;
    min-width: 80rpx;
    text-align: right;
}

.rightContent {
    height: 132rpx;
    background: #F7F7F9;
    border-radius: 12rpx;
    box-sizing: border-box;
    padding: 24rpx 32rpx;
    position: relative;
    /* display: flex; */
    /* align-items: flex-start; */
    /* justify-content: space-between; */
}

.rightContent::after {
    content: '';
    position: absolute;
    width: 16rpx;
    height: 16rpx;
    border-radius: 50%;
    background: #89A0EE;
    z-index: 1;
    top: 58rpx;
    left: -48rpx;
}

.rightContent::before {
    content: '';
    position: absolute;
    width: 4rpx;
    height: 132rpx;
    background: #386DF3;
    border-radius: 2rpx;
    opacity: 0.25;
    z-index: 1;
    top: 82rpx;
    left: -42rpx;
}

.rightContent.last-one::before {
    display: none;
}

.rightContent-top {
    display: flex;
    justify-content: space-between;
    margin-bottom: 6rpx
}


